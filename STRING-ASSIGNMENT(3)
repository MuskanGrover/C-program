//Displaying a character array character by  character using index values. 
//Changing the character array by assigning  new values to respective index positions  and displaying the character array after  changes.
#include<stdio.h>
int main()
{
	char name[10]="MUSKAN G";
	printf("%c",name[0]);
	printf("\n%c",name[1]);
	printf("\n%c",name[2]);
	printf("\n%c",name[3]);
	printf("\n%c",name[4]);
	printf("\n%c",name[5]);
	printf("\n%c",name[6]);
	printf("\n%c",name[7]);
	
	name[2]='H';
	name[5]='U';
	printf("\n\nNAME AFTER CHANGES: %s",name);
	
	
}

