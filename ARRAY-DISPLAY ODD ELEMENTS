//Accept values in a 1-D array at run-time and display the ODD elements stored in it.
#include<stdio.h>
int main()
{
	int n;
	printf("ENTER THE VALUE OF N : ");
	scanf("%d",&n);
	int arr[n];
	printf("ENTER VALUES OF ARRAY ");
	for(int i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	for(int i=0;i<n;i++)
	{
		if(arr[i]%2!=0)
		{
			printf("\n%d\t",arr[i]);
		}
	}
	return 0;
	
}
