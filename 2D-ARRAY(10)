//Program to PRINT SUM OF ARRAY ELEMENTS
#include <stdio.h>

int main() {
    int arr[3][3] = {
        {7, 8, 6},
        {2, 7, 3},
        {4, 7, 9}
    };

    int sum = 0;

    for (int i = 0; i <3 ; i++) {
        for (int j = 0; j < 3; j++) {
            sum += arr[i][j];
        }
    }

    printf("Sum of all elements: %d\n", sum);

    return 0;
}
