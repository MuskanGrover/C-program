//Change/Replace every element to its square in a 1-D array of integers.
#include<stdio.h>
int main()
{
	int n;
	printf("ENTER THE VALUE OF SIZE OF ARRAY: ");
	scanf("%d",&n);
	int arr[n];
	printf("\nENTER VALUE OF ARRAY: ");
	for(int i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	for(int i=0;i<n;i++)
	{
		arr[i]=arr[i]*arr[i];
		printf("%d \t",arr[i]);
	}
}
	
