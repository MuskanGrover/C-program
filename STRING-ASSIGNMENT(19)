// Display the count of lowercase vowels in  a string entered by the user. 
//Use for loop to traverse and == operator  to compare.
#include<stdio.h>
int main()
{
	char name[15]="MUSKAN grover";
	int count=0;
	for(int i=0;name[i]!='\0';i++)
	{
		if(name[i]=='a' || name[i]=='e'||  name[i]=='i' || name[i]=='o' || name[i]=='u')
		{
			count ++;
		}
	}
	printf("\n%d",count);
}
